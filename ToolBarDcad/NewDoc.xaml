<Window x:Class="ToolBarDcad.NewDoc"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ToolBarDcad"
        mc:Ignorable="d"
        Title="Nouveau document" Height="170" Width="200" ResizeMode="NoResize" WindowStartupLocation="CenterScreen" Icon="Icones/NewDoc.ico" Topmost="True">
	<Grid>
		<ComboBox Margin="0,20,0,0" VerticalAlignment="Top" Width="120" Name="CabinCombo">
            <ComboBoxItem Content="Primo" />
            <ComboBoxItem Content="Primo exel" />
			<ComboBoxItem Content="Duo" />
            <ComboBoxItem Content="Duo exel" />
            <ComboBoxItem Content="Brio" />
            <ComboBoxItem Content="Brio exel" />
            <ComboBoxItem Content="Hauzeo" />
            <ComboBoxItem Content="Bambino" />
            <ComboBoxItem Content="Exéléo" />
		</ComboBox>
	    <ComboBox Margin="0,60,0,0" VerticalAlignment="Top" Width="120" Name="MaterialCombo">
		    <ComboBoxItem Content="Nylon" />
		    <ComboBoxItem Content="Aluminium" />
		    <ComboBoxItem Content="Inox" />
		</ComboBox>
		<Button Width="42" Height="22" VerticalAlignment="Bottom" Margin="0,0,0,15" Content="OK" Name="OkButton"/>
	</Grid>
</Window>
